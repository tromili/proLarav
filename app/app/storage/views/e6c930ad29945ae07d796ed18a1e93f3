              $i=0;
              $j=0;
              foreach($pgso as $pgs){
                if ($coti->idcotizacion==$pgs->idcotizacion) {
                  $i++;
                  if ($pgs->estado==1) {
                    $j++;
                  }
                }
              }
              if($i!=0){
                echo number_format(($j*100)/$i, 2, ",", ".").'%';  
              }else{
                echo 0;
              }
              
